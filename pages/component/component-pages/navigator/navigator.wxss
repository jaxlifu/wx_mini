.container {
    height: 100%;
    display: flex;
    flex-direction: column;
    align-items: center;
    justify-content: space-between;
    padding: 200rpx 0;
    box-sizing: border-box;
}

.textStyle {
    display: flex;
    flex-direction: column;
    align-items: center;
}

.textContainer {
    margin-top: 200px;
}


/** 修改默认的navigator点击态 **/

.navigator-hover {
    color: blue;
}


/** 自定义其他点击态样式类 **/

.other-navigator-hover {
    color: red;
}